  },
  "author": "nsvegur",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^20.5.1",
    "@vercel/node": "^2.15.10",
    "drizzle-kit": "^0.19.13",
    "pre-commit": "^1.2.2",
    "rimraf": "^5.0.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@planetscale/database": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^14.2.0",
    "drizzle-orm": "^0.28.2",
    "express": "^4.17.2"
